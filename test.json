//# json
{
  "$schema": "https://raw.githubusercontent.com/chenrz925/geyser/main/geyser_schema.json",
  "__compose__": {
    "path": {
      "__reference__": "geyser.composable.utils.PathManager"
    },
    "env": {
      "__reference__": "geyser.composable.utils.EnvManager"
    },
    "info": {
      "__reference__": "geyser.composable.utils.RuntimeInfo"
    }
  },
  "__execute__": {
    "__rule__": "linear",
    "__action__": [
      {
        "__name__": "info",
        "__enable__": true
      }
    ]
  }
}